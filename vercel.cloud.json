{
  "version": 2,
  "name": "sports-crm",
  "builds": [
    {
      "src": "next.config.cloud.mjs",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/spor-okulu/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_APP_MODE": "cloud",
    "NEXT_PUBLIC_CO_DEV_ENV": "cloud"
  },
  "functions": {
    "src/pages/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "regions": ["fra1"],
  "framework": "nextjs"
}